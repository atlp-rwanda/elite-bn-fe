{
  "collectCoverage": true,
  "verbose": true,
  "testEnvironment": "jsdom",
  "coverageReporters": ["lcov", "text"],
  "coverageDirectory": "coverage",
  "testTimeout": 10000,
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(scss|sass|css)$": "identity-obj-proxy"
  }
}
